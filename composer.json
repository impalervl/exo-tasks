{
  "name": "impalervl/exo-tests",
  "description": "Test tasks implementation wrapped in a simple PHP app.",
  "type": "project",
  "require": {
    "php": "^8.3",
    "symfony/http-foundation": "^7.2",
    "fakerphp/faker": "^1.24",
    "vlucas/phpdotenv": "^5.6",
    "exads/ab-test-data": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    },
    "files": [
      "core/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "symfony/http-client": "^7.2",
    "mockery/mockery": "^1.6"
  }
}
